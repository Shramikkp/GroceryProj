@model IEnumerable<GroceryPR.Models.UserTbl>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid">
    <!-- OVERVIEW -->
    <div class="panel panel-headline">
        <div class="panel-heading">
            <h3 class="panel-title">Users</h3>
        </div>
        <div class="panel-body">
            <div class="row">
                <table class="table">
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.FirstName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.LastName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.MobileNumber)
                        </th>
                        
                        <th>
                            @Html.DisplayNameFor(model => model.Email)
                        </th>
                        
                        <th>
                            @Html.DisplayNameFor(model => model.Landmark)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.SocityName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.WingName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.FlatNumber)
                        </th>
                        
                        <th>
                            @Html.DisplayNameFor(model => model.CityTbl.Name)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.CountryTbl.Name)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.StateTbl.Name)
                        </th>
                        @*<th></th>*@
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.FirstName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.LastName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.MobileNumber)
                            </td>
                           
                            <td>
                                @Html.DisplayFor(modelItem => item.Email)
                            </td>
                           
                            <td>
                                @Html.DisplayFor(modelItem => item.Landmark)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.SocityName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.WingName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.FlatNumber)
                            </td>
                           
                            <td>
                                @Html.DisplayFor(modelItem => item.CityTbl.Name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.CountryTbl.Name)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.StateTbl.Name)
                            </td>
                            @*<td>
                                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                            </td>*@
                        </tr>
                    }

                </table>
            </div>         
        </div>
    </div>
    <!-- END OVERVIEW -->
</div>


